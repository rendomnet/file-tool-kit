{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@rendomnet/file-tool-kit/*": ["../dist/*"]
    }
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}
